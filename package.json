{
  "name": "voke",
  "version": "1.0.0",
  "description": "A modern BBCode editor with integrated image hosting",
  "author": "Voke Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev-backend": "nodemon server/dev-proxy.js",
    "dev": "concurrently \"npm:dev-backend\" \"vite\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "sync-css": "node scripts/sync-css.js",
    "prebuild": "npm run sync-css",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:server": "cd server && npm run lint",
    "lint:all": "npm run lint && npm run lint:server",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "browser-image-compression": "^2.0.2",
    "sceditor": "^3.2.0",
    "vite-plugin-commonjs": "^0.10.4",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0"
  }
}
